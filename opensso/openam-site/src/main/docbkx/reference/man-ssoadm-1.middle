     <term><option>--modulename, -o</option></term>
     <listitem><para>Module Name.</para></listitem>
    </varlistentry>
    <varlistentry>
     <term><option>--name, -m</option></term>
     <listitem><para>Name of authentication configuration.</para></listitem>
    </varlistentry>
    <varlistentry>
     <term><option>--password-file, -f</option></term>
     <listitem><para>File name that contains password of administrator.</para></listitem>
    </varlistentry>
    <varlistentry>
     <term><option>--realm, -e</option></term>
     <listitem><para>Name of realm.</para></listitem>
    </varlistentry>
    <varlistentry>
     <term><option>[--options, -t]</option></term>
     <listitem><para>Options for this entry.</para></listitem>
    </varlistentry>
