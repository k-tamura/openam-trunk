ity.openid.provider.DefineHandler</handler-class>  </tag>  <!-- function: concatenates two strings -->  <function>    <function-name>concat</function-name>    <function-class>com.sun.identity.openid.provider.Functions</function-class>    <function-signature>java.lang.String concat(java.lang.String, java.lang.String)</function-signature>  </function>  <!-- outputs a diagnostic message to standard error stream -->  <function>    <function-name>diag</function-name>    <function-class>com.sun.identity.openid.provider.Functions</function-class>    <function-signature>void diag(java.lang.String)</function-signature>  </function>  <!-- escapes text to prevent cross-site scripting vulnerability -->  <function>    <function-name>escape</function-name>    <function-class>com.sun.identity.openid.provider.Functions</function-class>    <function-signature>java.lang.String escape(java.lang.String)</function-signature>  </function>  <!-- escapes a URL and wraps it with an anchor -->  <function>    <function-name>externalLink</function-name>    <function-class>com.sun.identity.openid.provider.Functions</function-class>    <function-signature>java.lang.String externalLink(java.lang.String)</function-signature>  </function>  <!-- returns size of the supplied map -->  <function>    <function-name>size</function-name>    <function-class>com.sun.identity.openid.provider.Functions</function-class>    <function-signature>int size(java.util.Map)</function-signature>  </function>  <!-- splits a string around matches of a given regular expression -->  <function>    <function-name>split</function-name>    <function-class>com.sun.identity.openid.provider.Functions</function-class>    <function-signature>java.lang.String[] split(java.lang.String, java.lang.String)</function-signature>  </function>  <!-- emboldens text -->  <function>    <function-name>strong</function-name>    <function-class>com.sun.identity.openid.provider.Functions</function-class>    <function-signature>java.lang.String strong(java.lang.String)</function-signature>  </function></facelet-taglib>