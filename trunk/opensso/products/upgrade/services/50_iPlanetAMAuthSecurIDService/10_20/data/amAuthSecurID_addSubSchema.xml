<!DOCTYPE Requests
    PUBLIC "-//iPlanet//Sun Java System Access Manager 2005Q4 Admin CLI DTD//EN"
"jar://com/iplanet/am/admin/cli/amAdmin.dtd"
>
<!--
  this file is referenced by:

  amadmin -u "xxx" -w "yyy" -t amAuthSecurID_mod.xml

-->

<Requests>
    <SubSchema name="serverconfig" inheritance="multiple">
	<AttributeSchema name="iplanet-am-auth-securid-config-port"
	    type="single"
	    syntax="number"
	    i18nKey="a5">
	    <DefaultValues>
		<Value>58943</Value>
	    </DefaultValues>
	</AttributeSchema>

	<AttributeSchema name="iplanet-am-auth-securid-helper-port"
	    type="single"
	    syntax="number"
	    i18nKey="a6">
	    <DefaultValues>
		<Value>57943</Value>
	    </DefaultValues>
	</AttributeSchema>

	<AttributeSchema name="iplanet-am-auth-securid-server-config-path"
	    type="single"
	    syntax="string"
	    i18nKey="a1">
	    <DefaultValues>
		<Value>/opt/ace/data</Value>
	    </DefaultValues>
	</AttributeSchema>
	<AttributeSchema name="iplanet-am-auth-securid-auth-level"
 	    type="single"
	    syntax="number"
	    i18nKey="a900">
	    <DefaultValues>
		<Value>0</Value>
	    </DefaultValues>
	</AttributeSchema>
    </SubSchema>

</Requests>
