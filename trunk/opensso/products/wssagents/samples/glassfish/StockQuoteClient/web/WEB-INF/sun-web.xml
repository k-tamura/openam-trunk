<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 9.0 Servlet 2.5//EN" "http://www.sun.com/software/appserver/dtds/sun-web-app_2_5-0.dtd">
<sun-web-app error-url="" httpservlet-security-provider="FAMHttpProvider">
  <context-root>/StockQuoteClient</context-root>
  <security-role-mapping>
    <role-name>AUTHENTICATED_USERS</role-name>
    <principal-name class-name="com.sun.identity.wss.security.SecurityPrincipal">AUTHENTICATED_USERS</principal-name>
  </security-role-mapping>
  <service-ref>
    <service-ref-name>com.samples.GetQuote/service</service-ref-name>
    <port-info>
      <wsdl-port>
        <namespaceURI>http://sun.com/stockquote.wsdl</namespaceURI>
        <localpart>StockQuotePortTypePort</localpart>
      </wsdl-port>
    </port-info>
  </service-ref>
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="classdebuginfo" value="true">
      <description>Enable debug info compilation in the generated servlet class</description>
    </property>
    <property name="mappedfile" value="true">
      <description>Maintain a one-to-one correspondence between static content and the generated servlet class' java code</description>
    </property>
  </jsp-config>
</sun-web-app>
